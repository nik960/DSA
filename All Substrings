
public class Solution {

	public static void printSubstrings(String str) {
		//Your code goes here
        int p=0;
        
        for(int i=0;i<str.length();i++)
        {
            p=i+1;
            for(int j=i;j<str.length();j++){
        
        
            System.out.println(str.substring(i,p));
            p++;
        }
        }
	}

}
