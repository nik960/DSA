
public class Solution {  

    public static void insertionSort(int[] arr) {
    	//Your code goes here
        int tmp;
        for(int i=1;i<arr.length;i++)
        {
            tmp=arr[i];
            
            int j=i-1;
            
            while(j>=0 && arr[j]>tmp)
            {
                arr[j+1]=arr[j];
                j--;
            }
            arr[j+1]=tmp;
				        
            
        }
    }

}
